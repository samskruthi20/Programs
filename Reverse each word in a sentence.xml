import java.util.Arrays;


public class Main{
    
    public static void main(String [] args){
        

    String s = "Java is fun";
    
    String sentrev ="";
    
    String[] words = s.split(" ");
  
  for(String word : words){
      
      String rev = "";
      
      for(int i=word.length()-1 ; i>=0; i--){
          
         char c= word.charAt(i);
         rev = rev+c;
          
      }
      
      sentrev = sentrev + rev + " ";
      
  }
    
    System.out.println(sentrev);
   }

 }
    