public class Main {

    public static void main(String[] args) {

        int num = 121;

        int rem;
        int result = 0;

        int original = num;

        while (num != 0) {

            rem = num % 10;          
            result = result * 10 + rem; 
            num = num / 10;           
        }

        if (original == result) {
            System.out.println("number is palindrome");
        } else {
            System.out.println("number is not palindrome");
        }
    }
}
