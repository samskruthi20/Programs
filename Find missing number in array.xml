class Main {
    public static void main(String[] args) {
        
        int[] a = {1,2,3,4,6};
       
        int n = a.length + 1;
       
        int expectedSum = n * (n + 1) / 2;
        int actual = 0;
       
        for (int arr : a) {
            actual += arr;
        }
       
        int miss = expectedSum - actual;
        System.out.println(miss);
    }
}
