class Main {
    public static void main(String[] args) {
       
   int[] a = {3,5,4,32,6,5,9};
   
   int first = 0;
   int last = a.length-1;
   int temp;
   
   while(first<last){
       
       temp=a[first];
       a[first]=a[last];
       a[last]=temp;
       
       first++;
       last--;
   }
   
   for(int i=0; i<a.length; i++){
       
       System.out.print(a[i]+ " ");
   }
   
    }
}