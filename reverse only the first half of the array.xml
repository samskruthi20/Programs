class Main {
    public static void main(String[] args) {
       
       int[] a = {2,4,3,2,3,4,5,6,8};
       
      int first = 0;
      int middle = (a.length/2)-1;
      int temp;
      
      while(first<middle){
           temp=a[first];
           a[first] = a[middle];
           a[middle] = temp;
           
           first++;
           middle--;
          
      }
      
     for (int num : a) {
    System.out.print(num + " ");
}
    }
}