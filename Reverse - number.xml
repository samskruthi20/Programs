public class Main {
    public static void main(String[] args) {

        int num = 121;
        int rem;
        int result = 0;

        while (num != 0) {
            rem = num % 10;        
            result = result * 10 + rem;   
            num = num / 10;       
        }

        System.out.println(result);
    }
}
